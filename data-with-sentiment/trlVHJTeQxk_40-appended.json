        },
        {
            "body": "\u52d5\u753b\u9014\u4e2d\u306e\u30ab\u30fc\u30c9\u306b\n\u4e7e\u3055\u3093\u306e\u30c1\u30e3\u30f3\u30cd\u30eb\u3078\u306e\n\u30ea\u30f3\u30af\u3092\u5165\u308c\u308b\u3042\u305f\u308a\u304c\u597d\u304d",
            "polarity": 0.0,
            "subjectivity": 0.0
        },
        {
            "body": "\u300c\u5272\u308a\u5207\u308c\u306a\u3044\u3051\u3069\u4f59\u308a\u3058\u3083\u306a\u3044\n\u305d\u3063\u3068\u671d\u307e\u3067\u901a\u305b\u3093\u307c\u300d\n\u7518\u3044\u58f0\u304b\u3089\u610f\u5730\u60aa\u305d\u3046\u306a\u7537\u306e\u58f0\u306b\u5909\u308f\u308b\u3053\u306e\u90e8\u5206\u304c\u4e00\u756a\u30af\u308b",
            "polarity": 0.0,
            "subjectivity": 0.0
        },
        {
            "body": "\u5207\u5b9f\u306b\u306a\u3093\u3067\u3082\u3044\u3044\u304b\u3089\u3044\u3064\u304bLEO\u69d8\u3068\u6b4c\u3063\u3066\u6b32\u3057\u3044",
            "polarity": 0.0,
            "subjectivity": 0.0
        },
        {
            "body": "\u7ae5\u9854\u3067\u5de8\u4e73\u306b\u306a\u308b\u3063\u3066\u4e00\u4f53\u8ab0\u306e\u8840\u3092\u5438\u3063\u305f\u3089\u305d\u3046\u306a\u308b\u306e\u304b\u2026\n\u6bcd\u89aa\u304b\uff1f",
            "polarity": 0.0,
            "subjectivity": 0.0
        },
        {
            "body": "\u9045\u308c\u3070\u305b\u306a\u304c\u3089\u6982\u8981\u6b04\u306e\u4ffa/\u845b\u8449\u69d8\u306b\u306b\u3063\u3053\u308a\u3057\u307e\u3057\u305f \u7b11\n\u7d20\u6575\u306a\u6b4c\u58f0\u3067\u8133\u5185\u518d\u751f\u304c\u6b62\u307e\u308a\u307e\u305b\u3093\uff01",
            "polarity": 0.0,
            "subjectivity": 0.0
        },
        {
            "body": "\u845b\u8449\u3068\u30b5\u30fc\u30cb\u30e3\u304c\u4e26\u3093\u3067\u308b\u3068\u3053\u898b\u305f\u3044",
            "polarity": 0.0,
            "subjectivity": 0.0
        },
        {
            "body": "\u3053\u306e\u5973\u306e\u5b50\u3068\u3069\u3053\u3067\u7d50\u5a5a\u3067\u304d\u307e\u3059\u304b",
            "polarity": 0.0,
            "subjectivity": 0.0
        },
        {
            "body": "\u8868\u60c5\u5c11\u3057\u305a\u3064\u5909\u308f\u3063\u3066\u308b\u306e\u304c\u4ffa\u306b\u523a\u3055\u308a\u3059\u304e\u308b",
            "polarity": 0.0,
            "subjectivity": 0.0
        },
        {
            "body": "\u6700\u521d\u306e\u3000\"\u3042\u305f\u3057\u30f4\u30a1\u30f3\u30d1\u30a4\u30a2\"\u3000\u3067\u4f55\u304b\u3042\u308f\u308f\u308f\u308f\u3063\u3066\u306a\u3063\u305f\u7b11\u7b11",
            "polarity": 0.0,
            "subjectivity": 0.0
        },
        {
            "body": "\u30ad\u30e3\u30e9\u30bd\u30f3\u611f\u306e\u3042\u308b\u6b4c\u3044\u65b9\u304c\u300c\u3053\u3048\u3067\u304a\u3057\u3054\u3068\u300d\u3092\u30ac\u30c3\u30c4\u30ea\u3068\u5f15\u304d\u7acb\u3066\u3066\u304f\u308c\u3066\u611f\u8b1d\u611f\u6fc0 \u2026\u2026 \uff08\u8a9e\u5f59\u9727\u6563\uff09",
            "polarity": 0.0,
            "subjectivity": 0.0
        },
        {
            "body": "\u9014\u4e2d\u3067\u4e7e\u6bbf\u306e\u30c1\u30e3\u30f3\u30cd\u30eb\u3067\u3066\u304f\u308b\u306e\u6700\u9ad8\u306b\u30a8\u30e2\u3060\u306a",
            "polarity": 0.0,
            "subjectivity": 0.0
        },
        {
            "body": "\u30ad\u30df\u53ef\u611b\u3044\u306d\u3001\u3069\u3053\u4f4f\u307f\uff1f",
            "polarity": 0.0,
            "subjectivity": 0.0
        },
        {
